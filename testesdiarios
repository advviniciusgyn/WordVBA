        xlFile.Save
        xlFile.Close
        appXL.Quit
        Set appXL = Nothing
        Application.DisplayAlerts = True
        '--Officetricks Code Changes End Here
       With ActiveDocument.Content.Find
            .Forward = True
            .Wrap = wdFindStop
            .Execute FindText:="nao_gravado", replacewith:="gravado em " & Now, Replace:=wdReplaceAll, MatchCase:=True
        End With
    
    MsgBox "Informações processuais gravadas com sucesso.", vbInformation, "SAP informa:"
    Else
    End If

    
