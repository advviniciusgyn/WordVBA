Private Sub Dataprotocolo_Change()
Datapublicado.Enabled = True
Datapublicado.BackColor = vbWhite
End Sub

Private Sub Datapublicado_AfterUpdate()
Dim Dataprotocolo As Date
Dim Datapublicado As Date
Dim resultdata As Integer
resultdata = DateDiff("d", Datapublicado, Dataprotocolo)
TxtBox_qtdias.Value = resultdata
End Sub



