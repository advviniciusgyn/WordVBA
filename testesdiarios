Private Sub OptionButton1_AT_Click()
If OptionButton1_AT = True Then
Cargoassinaatobox.Enabled = False
Nomeassinatobox.Enabled = False
Cargoassinaatobox.BackColor = RGB(204, 204, 204)
Nomeassinatobox.BackColor = RGB(204, 204, 204)
Label18.ForeColor = RGB(204, 204, 204)
Label19.ForeColor = RGB(204, 204, 204)
Else
End If
End Sub

Private Sub OptionButton2_AT_Click()
If OptionButton2_AT = True Then
Cargoassinaatobox.Enabled = False
Nomeassinatobox.Enabled = False
Cargoassinaatobox.BackColor = RGB(204, 204, 204)
Nomeassinatobox.BackColor = RGB(204, 204, 204)
Label18.ForeColor = RGB(204, 204, 204)
Label19.ForeColor = RGB(204, 204, 204)
Else
End If
End Sub

Private Sub OptionButton3_AT_Click()
If OptionButton3_AT = True Then
Nomeassinatobox.Enabled = True
Cargoassinaatobox.Enabled = True
Cargoassinaatobox.BackColor = vbWhite
Nomeassinatobox.BackColor = vbWhite
Label18.ForeColor = vbBlack
Label19.ForeColor = vbBlack
Nomeassinatobox.SetFocus
Else
Cargoassinaatobox.Enabled = False
Nomeassinatobox.Enabled = False
Cargoassinaatobox.BackColor = RGB(204, 204, 204)
Nomeassinatobox.BackColor = RGB(204, 204, 204)
Label18.ForeColor = RGB(204, 204, 204)
Label19.ForeColor = RGB(204, 204, 204)
End If
End Sub

Private Sub OptionButton1_TA_Click()
If OptionButton1_TA = True Then
Tipoatobox.Enabled = False
Tipoatobox.BackColor = RGB(204, 204, 204)
Label20.ForeColor = RGB(204, 204, 204)
Else
End If
End Sub

Private Sub OptionButton2_TA_Click()
If OptionButton2_TA = True Then
Tipoatobox.Enabled = False
Tipoatobox.BackColor = RGB(204, 204, 204)
Label20.ForeColor = RGB(204, 204, 204)
Else
End If
End Sub

Private Sub OptionButton3_TA_Click()
If OptionButton3_TA = True Then
Tipoatobox.Enabled = True
Tipoatobox.BackColor = vbWhite
Label20.ForeColor = vbBlack
Tipoatobox.SetFocus
Else
Tipoatobox.Enabled = False
Tipoatobox.BackColor = RGB(204, 204, 204)
Label20.ForeColor = RGB(204, 204, 204)
End If
End Sub



