Private Sub UserForm_Initialize()
'Define em qual aba vai inicializar
MultiPage1.Value = 0

'Inserindo opções no combobox
'Anos do processo
With Periodobox
        .AddItem ("2015")
        .AddItem ("2016")
        .AddItem ("2017")
        .AddItem ("2018")
        .AddItem ("2019")
        .AddItem ("2020")
        .AddItem ("2021")
        .AddItem ("2022")
        .AddItem ("2023")
        .AddItem ("2024")
        .AddItem ("2025")
End With

'Nome do servidor
With Cmboxservidor
        .AddItem ("Samira de Oliveira Rezende")
        .AddItem ("Cristiane Monteiro Machado")
        .AddItem ("Claudia Alessandra da Silva")
        .AddItem ("Rosa Maria Caiado Cunha e Cruz")
        .AddItem ("Ilma Aparecida Silva")
        .AddItem ("Davi de Castro Batista")
        .AddItem ("Luís Paulo Carmo de Melo")
        .AddItem ("José Ronaldo de Sene")
        .AddItem ("Douglas Santos")
        .AddItem ("Lilian Oliveira Veras Ribeiro")
        .AddItem ("Juliana da Silva Vinhal Andrade")
        .AddItem ("Gleys Regina Alves de Paiva Lopes")
        .AddItem ("Kátia Cristina Jardim de Sousa")
        .AddItem ("Flávia Lemes da Silva")
End With

'Estágio da análise
With ComboBox_estagiodaanalise
        .AddItem ("Primeira análise")
        .AddItem ("Segunda análise")
        .AddItem ("Terceira análise")
        .AddItem ("Quarta análise")
        .AddItem ("Mais de cinco análises")
End With

'Tipos de aposentadoria
With CBox_regrasap
        .AddItem ("Idade e tempo de contrib. (art. 40, § 1º, III e ‘a’ da CF)")
        .AddItem ("Idade e tem. de contrib. professor (art. 40, §5º da CF)")
        .AddItem ("Idade (art. 40, § 1º, III e ‘b’ da CF)")
        .AddItem ("Invalidez integrais pela média (40, § 1º, I da CF)")
        .AddItem ("Invalidez proporcionais pela média (40, § 1º, I da CF)")
        .AddItem ("Regra de transição art. 6º da EC 41/03")
        .AddItem ("Regra de transição art. 6º da EC 41/03 para professor")
        .AddItem ("Regra de transição art. 3º da EC 47/05")
        .AddItem ("Invalidez com integralidade (EC 70/12)")
        .AddItem ("Invalidez com proventos proporcionais (EC 70/12)")
        .AddItem ("Aposentadoria compulsória (art. 40, II da CF)")
        .AddItem ("Aposentadoria Especial (Art. 40, §1º, III da CF)")
End With

'Gênero do aposentado
With CB_sexo
        .AddItem ("Homem")
        .AddItem ("Mulher")
End With

'Integralidade
With CBox_doecaintegral
        .AddItem ("Sim")
        .AddItem ("Não")
End With

'Integralidade
With CBox_previalicenca
        .AddItem ("Sim")
        .AddItem ("Não")
End With

'Inserindo opções no combobox
Application.ScreenUpdating = False
Application.DisplayAlerts = False
Set appXL = CreateObject("Excel.Application")
Set xlFile = appXL.Workbooks.Open("F:\Auditorias\2019\PES\Banco de Gestores.xlsx")
Cmboxmunicipio.List = xlFile.Worksheets(1).Range("A2:A175").Value
 xlFile.Close
appXL.Quit
Set appXL = Nothing
Application.ScreenUpdating = True
Application.DisplayAlerts = True


'Inserindo imagem com a meta do mês
Meta.Picture = LoadPicture("F:\Auditorias\2019\PES\35\Admininstrativo 2019\Produtividade 2019\Meta do mês.bmp")

'Inserindo imagem com distribuição de processos
Distribuicao.Picture = LoadPicture("F:\Auditorias\2019\PES\35\Admininstrativo 2019\Outros\Distribuição de processos.bmp")

End Sub


