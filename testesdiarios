Private Sub CheckBox_1__Click()
If CheckBox_1_ = True Then
CheckBox_1_1_.Enabled = True
CheckBox_1_2_.Enabled = True
CheckBox_1_3_.Enabled = True
    Else
CheckBox_1_1_.Enabled = False
CheckBox_1_2_.Enabled = False
CheckBox_1_3_.Enabled = False
    End If
End Sub

Private Sub CheckBox_2_1_2_1_Click()
If CheckBox_2_1_2_1 = True Then
Frame_defesa.Visible = True
Frame38.Visible = True
Frame39.Visible = False
Label_defesa.Visible = True
TxtBox_defesaintemp.Visible = True
CheckBox_2_1_2_2 = False
    Else
Frame_defesa.Visible = False
CheckBox_2_1_2_2 = True
    End If
End Sub

Private Sub CheckBox_2_1_2_2_Click()
If CheckBox_2_1_2_2 = True Then
Frame_defesa.Visible = True
Frame39.Visible = True
Frame38.Visible = False
Label_defesa.Visible = False
TxtBox_defesaintemp.Visible = False
CheckBox_2_1_2_1 = False
    Else
Frame_defesa.Visible = False
CheckBox_2_1_2_1 = True
    End If
End Sub

