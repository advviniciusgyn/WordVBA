'INSERÇÃO DE DADOS NO EXCEL
Pergunta4 = MsgBox("Deseja GRAVAR informações no controle de processos?", vbYesNo, "SAP")
    If Pergunta4 = vbYes Then
        'To update in database excel file | enable "microsoft excel xx object library" in Tools -> references
        Dim appXL As Object, xlFile As Object, oWsht As Object
        Set appXL = CreateObject("Excel.Application")
        Set xlFile = appXL.Workbooks.Open("F:\Auditorias\2019\PES\Banco de dados de aposentadoria 2019 - 2º Semestre.xlsx") 'update the database file path
        Set oWsht = xlFile.Sheets("Planilha1") 'change the sheet name if you change it in database file
        
        Dim lastrow As Long
        'lastrow = oWsht.Range("A" & Rows.Count).End(xlUp).Row + 1 'Finds the last blank row
        lastrow = 2
        Do While oWsht.Range("A" & lastrow) <> ""
            lastrow = lastrow + 1
        Loop
        'Store Values in Database
        Application.DisplayAlerts = False
        oWsht.Range("A" & lastrow).Value = Proc_numero
        oWsht.Range("B" & lastrow).Value = Periodo_ano
        oWsht.Range("C" & lastrow).Value = Orgao_entidade
        oWsht.Range("D" & lastrow).Value = Nome_Gestor
        oWsht.Range("E" & lastrow).Value = Cargo_Gestor
