Private Sub CB_salvar_Click()
'TELA 0
'NÚMERO DO PROCESSO
Proc_numero = Numprobox
    'Criando o critério de pesquisa
With ActiveDocument.Content.Find
    .Forward = True
    .Wrap = wdFindStop
    .Execute FindText:="PROCNUMERO1", replacewith:=Proc_numero, Replace:=wdReplaceAll, MatchCase:=True
End With

'PERÍODO
