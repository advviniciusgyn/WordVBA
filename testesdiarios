End If
End Sub

Private Sub CB_ant6_Click()
If CheckBox_mudacargo = True Then
MultiPage1.Value = 4
ElseIf Cmboxmunicipio = "GOIANIA" Then
MultiPage1.Value = 3
Else
MultiPage1.Value = 2
End If
End Sub

Private Sub CB_ant7_Click()
If CheckBox_produtfiscal = True Then
MultiPage1.Value = 5
ElseIf CheckBox_mudacargo = True Then
MultiPage1.Value = 4
ElseIf Cmboxmunicipio = "GOIANIA" Then
MultiPage1.Value = 3
Else: MultiPage1.Value = 2
End If
End Sub



