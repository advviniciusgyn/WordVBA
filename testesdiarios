Private Sub Nomegestorbox_Change()
If Nomegestorbox.Value = "" Then
Nomegestorbox.BackColor = vbRed
Nomegestorbox.SetFocus
Else
Nomegestorbox.BackColor = vbWhite
End If
End Sub

