        oWsht.Range("L" & lastrow).Value = Cargobenefbox
        oWsht.Range("M" & lastrow).Value = OptionButton1_AT
        oWsht.Range("N" & lastrow).Value = OptionButton2_AT
        oWsht.Range("O" & lastrow).Value = OptionButton3_AT
        oWsht.Range("P" & lastrow).Value = Nomeassinatobox
        oWsht.Range("Q" & lastrow).Value = Cargoassinaatobox
        oWsht.Range("R" & lastrow).Value = OptionButton1_TA
        oWsht.Range("S" & lastrow).Value = OptionButton2_TA
        oWsht.Range("T" & lastrow).Value = OptionButton3_TA
        oWsht.Range("U" & lastrow).Value = Tipoatobox
        oWsht.Range("V" & lastrow).Value = Numatoconcbox
        oWsht.Range("W" & lastrow).Value = Dataatoconcbox
        oWsht.Range("X" & lastrow).Value = Folhaatobox
        oWsht.Range("Y" & lastrow).Value = Parecercbox
        oWsht.Range("Z" & lastrow).Value = ComboBox_estagiodaanalise
        oWsht.Range("AA" & lastrow).Value = CkBox_retificacao
        oWsht.Range("AB" & lastrow).Value = OptionButton1_TA2
        oWsht.Range("AC" & lastrow).Value = OptionButton2_TA2
        oWsht.Range("AD" & lastrow).Value = OptionButton3_TA2
        oWsht.Range("AE" & lastrow).Value = Tipoatobox2
        oWsht.Range("AF" & lastrow).Value = Numatoconcbox2
        oWsht.Range("AG" & lastrow).Value = Folhaatobox2
        oWsht.Range("AH" & lastrow).Value = CkBox_retroatividade
        oWsht.Range("AI" & lastrow).Value = TxtBox_retroatividade
