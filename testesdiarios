Private Sub CBox_regrasap_Change()
'Primeiro limpa as opções
CBox_requisitodescump.Clear

'Mostrar a regra em outros lugares
TxtBox_mostraregra1.Text = CBox_regrasap

'Primeira regra Idade e tempo de contrib. (art. 40, § 1º, III e ‘a’ da CF)"
If CBox_regrasap = "Idade e tempo de contrib. (art. 40, § 1º, III e ‘a’ da CF)" Then
'Invisível
Frame_tempcarreira.Visible = False
Frame_tempmagis.Visible = False
Frame_tempcontribexcecido.Visible = False
Frame_requisitosinvalidez.Visible = False
Frame_descumprequisitosinvalidez.Visible = False
'Visível
Frame_dataingresso.Visible = True
Frame_tempcontrib.Visible = True
Frame_idade.Visible = True
Frame_temposervpub.Visible = True
Frame_tempcargo.Visible = True
Frame_requisitos.Visible = True
Frame_cumprequisitos.Visible = True
'Carregar hipóteses de descumprimento
With CBox_requisitodescump
        .AddItem ("Data de ingresso no serviço público em cargo efetivo")
        .AddItem ("Tempo de contribuição insuficiente")
        .AddItem ("Licença averbada em dobro depois de 98")
        .AddItem ("Desaverbação de parte do tempo de contribuição não utilizado prestado no município")
        .AddItem ("Reconhecimento de tempo do INSS sem CTC prestado no próprio município, sem averbação automática prévia")
        .AddItem ("Tempo de serviço público insuficiente")
        .AddItem ("Idade insuficiente")
        .AddItem ("Tempo de carreira insuficiente")
        .AddItem ("Tempo de cargo público insuficiente")
        .AddItem ("Tempo de magistério insuficiente")
        .AddItem ("Tempo de contribuição excedido do art. 3º da EC 47/05 insuficiente")
        .AddItem ("Ausência de documentos para comprovar requisitos")
        .AddItem ("Outro - reescrever aqui o requisito descumprido")
End With


'Regra "Idade e tem. de contrib. professor (art. 40, §5º da CF)"
ElseIf CBox_regrasap = "Idade e tem. de contrib. professor (art. 40, §5º da CF)" Then
'Invisível
Frame_tempcarreira.Visible = False
Frame_tempcontribexcecido.Visible = False
Frame_requisitosinvalidez.Visible = False
Frame_descumprequisitosinvalidez.Visible = False
'Visível
Frame_dataingresso.Visible = True
Frame_tempcontrib.Visible = True
Frame_idade.Visible = True
Frame_temposervpub.Visible = True
Frame_tempcargo.Visible = True
Frame_tempmagis.Visible = True
Frame_requisitos.Visible = True
Frame_requisitos2.Visible = True
Frame_cumprequisitos.Visible = True
'Carregar hipóteses de descumprimento
With CBox_requisitodescump
        .AddItem ("Data de ingresso no serviço público em cargo efetivo")
        .AddItem ("Tempo de contribuição insuficiente")
        .AddItem ("Licença averbada em dobro depois de 98")
        .AddItem ("Desaverbação de parte do tempo de contribuição não utilizado prestado no município")
        .AddItem ("Reconhecimento de tempo do INSS sem CTC prestado no próprio município, sem averbação automática prévia")
        .AddItem ("Tempo de serviço público insuficiente")
        .AddItem ("Idade insuficiente")
        .AddItem ("Tempo de carreira insuficiente")
        .AddItem ("Tempo de cargo público insuficiente")
        .AddItem ("Tempo de magistério insuficiente")
        .AddItem ("Tempo de contribuição excedido do art. 3º da EC 47/05 insuficiente")
        .AddItem ("Ausência de documentos para comprovar requisitos")
        .AddItem ("Outro - reescrever aqui o requisito descumprido")
End With

'Regra "Idade (art. 40, § 1º, III e ‘b’ da CF)"
ElseIf CBox_regrasap = "Idade (art. 40, § 1º, III e ‘b’ da CF)" Then
'Invisível
Frame_tempcontrib.Visible = False
Frame_tempcarreira.Visible = False
Frame_tempmagis.Visible = False
Frame_tempcontribexcecido.Visible = False
Frame_requisitosinvalidez.Visible = False
Frame_descumprequisitosinvalidez.Visible = False
'Visível
Frame_dataingresso.Visible = True
Frame_idade.Visible = True
Frame_temposervpub.Visible = True
Frame_tempcargo.Visible = True
Frame_requisitos.Visible = True
Frame_requisitos2.Visible = True
Frame_cumprequisitos.Visible = True
'Carregar hipóteses de descumprimento
With CBox_requisitodescump
        .AddItem ("Data de ingresso no serviço público em cargo efetivo")
        .AddItem ("Tempo de contribuição insuficiente")
        .AddItem ("Licença averbada em dobro depois de 98")
        .AddItem ("Desaverbação de parte do tempo de contribuição não utilizado prestado no município")
        .AddItem ("Reconhecimento de tempo do INSS sem CTC prestado no próprio município, sem averbação automática prévia")
        .AddItem ("Tempo de serviço público insuficiente")
        .AddItem ("Idade insuficiente")
        .AddItem ("Tempo de carreira insuficiente")
        .AddItem ("Tempo de cargo público insuficiente")
        .AddItem ("Tempo de magistério insuficiente")
        .AddItem ("Tempo de contribuição excedido do art. 3º da EC 47/05 insuficiente")
