'NOME DO ANALISTA
Nome_analista = Cmboxservidor
Naoassina1 = "Samira de Oliveira Rezende"
Naoassina2 = "Rosa Maria Caiado Cunha e Cruz"
Naoassina3 = "José Ronaldo de Sene"
Naoassina4 = "Cristiane Monteiro Machado"
    'Criando o critério de pesquisa
If Nome_analista = Naoassina1 Then
ActiveDocument.Bookmarks("Nome_analista").Range.Text = ""
ActiveDocument.Bookmarks("Cargo_analista").Range.Text = ""
ElseIf Nome_analista = Naoassina2 Then
ActiveDocument.Bookmarks("Nome_analista").Range.Text = ""
ActiveDocument.Bookmarks("Cargo_analista").Range.Text = ""
ElseIf Nome_analista = Naoassina3 Then
ActiveDocument.Bookmarks("Nome_analista").Range.Text = ""
ActiveDocument.Bookmarks("Cargo_analista").Range.Text = ""
ElseIf Nome_analista = Naoassina4 Then
ActiveDocument.Bookmarks("Nome_analista").Range.Text = ""
ActiveDocument.Bookmarks("Cargo_analista").Range.Text = ""
Else
With ActiveDocument.Content.Find
    .Forward = True
    .Wrap = wdFindStop
